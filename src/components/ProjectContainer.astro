---
import ProjectShowcase from "./ProjectShowcase.astro";
import type { ProjectShowcaseType } from "../types";

interface Props {
  src: Array<ProjectShowcaseType>;
}

const { src } = Astro.props;
---

<div class="flex flex-col justify-center items-center gap-4 mx-8">
  {src.map((p) => <ProjectShowcase src={p} />)}
</div>
