---
import ProjectShowcase from "./ProjectShowcase.astro";
import type { ProjectShowcase as ProjectShowcaseType } from "../const";

interface Props {
  src: Array<ProjectShowcaseType>;
}

const { src } = Astro.props;
---

<div class="flex justify-center items-start gap-4">
  {src.map((p) => <ProjectShowcase src={p} />)}
</div>
