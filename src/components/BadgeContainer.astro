---
import type { ExperienceShowcase } from "../const";
import ExperienceBadge from "./ExperienceBadge.astro";

interface Props {
  src: Array<ExperienceShowcase>;
}

const { src } = Astro.props;
---

<div class="flex flex-wrap justify-center gap-x-4 gap-y-6">
  {src.map((badge) => <ExperienceBadge src={badge} />)}
</div>
